
<div class="col-md-4 text-box">

	<div><%= @fileupload.section %></div>

	<br><br>

	<h1><%= @fileupload.title %></h1>

	<br><br>

	<div><%= @fileupload.description %></div>

	<br><br>

	<div><%= @fileupload.source %></div>

	<br><br>

	<div><%= @fileupload.bin %></div>

	<br><br>

	<%= link_to "Edit file", edit_fileupload_path(@fileupload) %>

	<%= link_to "Delete", fileupload_path(@fileupload), method: :delete, data: { confirm: "Are you sure you wish to delete this post?"} %>
	
</div>